	<Language_en_US>
		<Row Tag="TXT_KEY_LEADER_CALCTRUMBULL">
			<Text>Jonathan Trumbull</Text>
		</Row>
		<Row Tag="TXT_KEY_LEADER_CALCTRUMBULL_PEDIA">
			<Text>Jonathan Trumbull</Text>
		</Row>
		<!--<Row Tag="TXT_KEY_CIVILOPEDIA_LEADERS_CALCTRUMBULL">
